#include<iostream>
using namespace std;
class Book{
	private:
		string title;
		string author;
	public:
		Book(string t, string a)
		{
			title=t;
			author=a;
		}
		 Book(const Book &b){
			title=b.title;
			author=b.author;
		}
		void display()
		{
			cout<<"Author"<<author<<"title"<<title<<endl;
		}
};
int main()
{
	Book b1("bruno","vaishnavi ");
    Book b2=b1;
	b2.display();
	return 0;
}
